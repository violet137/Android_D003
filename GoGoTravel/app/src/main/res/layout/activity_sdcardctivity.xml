        android:id="@+id/gridsd" />

    <Button
        android:text="ok"
        android:layout_width="47dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|left"

        android:id="@+id/btnChuyen" />
</FrameLayout>
